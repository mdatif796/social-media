<header>
  <h2>Header</h2>
  <ul>
    <% if(locals.user){ %>
      <li><a href="/users/profile"><h2><%= user.name %> </h2></a></li>
      <li>
        <a href="/users/sign-out">Sign Out</a>
      </li> 
      <% } else{ %>
        <li>
          <a href="/users/sign-in">Sign In</a>
        </li> 
        <li>
          <a href="/users/sign-up">Sign Up</a>
        </li>
        <% } %> 
  </ul>  
</header>